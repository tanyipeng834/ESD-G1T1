{
    "name": "Python: Flask",
    "type": "python",
    "request": "launch",
    "module": "flask",
    "env": {
        "FLASK_APP": "inventory.py",
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "1",
        "dbURL" : "mysql+mysqlconnector://root@localhost:3306/inventory"
    },
    "args": [
        "run",
        "--no-debugger",
        "--no-reload"
    ],
    "jinja": true
}
